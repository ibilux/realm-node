{
  "name": "@ibilux/realm-node",
  "description": "Realm is a mobile database: an alternative to SQLite and key-value stores",
  "version": "11.0.3",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://realm.io",
  "keywords": [
    "database",
    "db",
    "react",
    "react-native",
    "persistence",
    "localstorage",
    "sqlite",
    "asyncstorage",
    "rocksdb",
    "leveldb",
    "realm"
  ],
  "author": "ibilux <i.bilux@gmail.com> (https://realm.io)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ibilux/realm-node.git"
  },
  "bugs": {
    "url": "https://github.com/ibilux/realm-node/issues"
  },
  "types": "types/index.d.ts",
  "main": "lib/index.js",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "install": "prebuild-install --tag-prefix= -r napi"
  },
  "dependencies": {
    "@realm/common": "^0.1.4",
    "prebuild-install": "^7.0.1",
    "bson": "4.4.1",
    "@realm/network-transport": "^0.7.2"
  },
  "engines": {
    "node": ">=13",
    "npm": ">=7"
  },
  "config": {
    "runtime": "napi",
    "target": 5
  },
  "binary": {
    "module_name": "realm",
    "host": "https://static.realm.io",
    "package_name": "realm-v11.0.0-napi-v5-{platform}-{arch}.tar.gz",
    "remote_path": "realm-js-prebuilds/11.0.0",
    "napi_versions": [
      5
    ]
  },
  "type": "commonjs",
  "directories": {
    "lib": "lib"
  }
